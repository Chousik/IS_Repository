          <stringProp name="cacheKey">true</stringProp>
          <stringProp name="script">import java.net.HttpURLConnection
import java.net.URL

def protocol = props.get("protocol") ?: "http"
def host = props.get("host") ?: "localhost"
def port = props.get("port") ?: "8080"

def sendDelete = { String path ->
    if (!path) {
        return
    }
    try {
        def url = new URL("${protocol}://${host}:${port}${path}")
        HttpURLConnection conn = (HttpURLConnection) url.openConnection()
        conn.setRequestMethod("DELETE")
        conn.setConnectTimeout(5000)
        conn.setReadTimeout(5000)
        conn.getResponseCode()
        try {
            conn.getInputStream()?.close()
        } catch (IOException ignore) {
            conn.getErrorStream()?.close()
        }
        conn.disconnect()
    } catch (Exception ignored) {
    }
}

def collect = []
["targetGroupId", "sharedGroupAId", "sharedGroupBId", "deleteGroupId"].each { key ->
    def value = props.get(key)
    if (value) {
        collect &lt;&lt; value
    }
}
def extra = props.get("exclusiveGroupId")
if (extra) {
    extra.split(",").each { collect &lt;&lt; it }
}
collect.each { sendDelete("/api/v1/study-groups/${it}") }

sendDelete("/api/v1/persons/${props.get('sharedAdminId')}")
sendDelete("/api/v1/persons/${props.get('exclusiveAdminId')}")
</stringProp>
